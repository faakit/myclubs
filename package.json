{
  "name": "api",
  "version": "0.0.1",
  "scripts": {
    "pretest": "yarn compile",
    "test": "NODE_ENV=test jest --verbose --forceExit --detectOpenHandles --passWithNoTests ",
    "test:ci": "yarn test -- --passWithNoTests --silent --noStackTrace --runInBand --coverage",
    "test:unit": "NODE_ENV=test jest --passWithNoTests --silent --noStackTrace --runInBand --watch -c jest-unit.config.js",
    "test:integration": "NODE_ENV=test jest --passWithNoTests --silent --noStackTrace --runInBand --watch -c jest-integration.config.js",
    "lint": "eslint 'src/**' '__tests__/**'",
    "lint:fix": "yarn lint -- --fix --quiet",
    "migrate": "yarn compile; yarn sequelize db:migrate",
    "migrate:staging": "yarn compile; NODE_ENV=staging yarn sequelize db:migrate",
    "predev": "yarn compile",
    "dev": "nodemon --watch 'src/**/*.ts' --ignore __tests__ --exec ts-node src/index.ts",
    "precompile": "rm -rf dist",
    "compile": "yarn tsc -p tsconfig-build.json",
    "preproduction": "yarn compile",
    "production": "NODE_ENV=production node dist/index.js",
    "prettier": "prettier --check ./",
    "prettier:fix": "prettier --check --write ./",
    "prepare": "is-ci || husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write",
      "git add ."
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/register": "^7.13.8",
    "axios": "^1.5.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "cpf-cnpj-validator": "^1.0.3",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "is-ci": "^3.0.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "node-polyglot": "^2.4.0",
    "sequelize": "^6.33.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/node": "^7.13.0",
    "@babel/preset-env": "^7.13.9",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@swc/core": "^1.3.57",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.33",
    "@types/cors": "^2.8.9",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.9",
    "@types/faker": "^5.5.9",
    "@types/jest": "^26.0.19",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.14",
    "@types/node-polyglot": "^2.4.1",
    "@types/sequelize": "^4.28.13",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.1",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "faker": "^5.5.3",
    "husky": "^5.1.3",
    "jest": "^29.5.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.0.1",
    "ts-node": "^7.0.1",
    "typescript": "^4.7.4"
  }
}
